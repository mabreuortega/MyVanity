@{
    H(T("Upload File"));
}

<div class="container">
    @using (Html.BeginForm("UploadFile", "About", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <div class="form-group">
            <p class="control-label col-md-2">Image: </p>
            <input type="file" id="file" name="file" />
        </div>

        <div class="form-group">
            <input type="submit" name="Submit" id="Submit" value="@T("Upload")" class="btn btn-default" />
        </div>
    }
</div>
